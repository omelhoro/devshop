{
  "name": "react-redux-typescript-webpack-starter",
  "version": "0.1.0",
  "description": "React / Redux / Typescript / Webpack - Starter",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (https://piotrwitek.github.io/)",
  "homepage": "https://piotrwitek.github.io/react-redux-typescript-webpack-starter/",
  "repository": "https://github.com/piotrwitek/react-redux-typescript-webpack-starter.git",
  "bugs": "https://github.com/piotrwitek/react-redux-typescript-webpack-starter/issues",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:prod": "npm run clean && webpack --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "source vars-dev.sh && webpack-dev-server --config configs/webpack.config.js",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js",
    "dev:awesome": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:watch": "jest --config configs/jest.json --watch",
    "test:update": "jest --config configs/jest.json -u",
    "reinstall": "rm -rf node_modules && npm",
    "precommit": "npm run lint",
    "prepush": "npm run lint & yarn run tsc & yarn run test",
    "deploy": "echo 'deploy not set!'",
    "graph": "tsc && madge --image graph.svg out/index.js --warning",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router-dom": "4.3.1",
    "@types/webpack": "4.4.22",
    "@types/webpack-env": "1.13.6",
    "bootstrap": "^4.1.3",
    "css-loader": "^2.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "3.2.0",
    "immer": "^1.7.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-snackbar": "^0.1.11",
    "popper.js": "^1.14.4",
    "postcss-loader": "^3.0.0",
    "query-string": "^6.1.0",
    "react": "16.7.0",
    "react-composer": "^5.0.1",
    "react-dom": "16.7.0",
    "react-request": "^3.1.2",
    "react-router-dom": "4.3.1",
    "reselect": "4.0.0",
    "rimraf": "2.6.2",
    "ts-loader": "5.3.2",
    "tslib": "1.9.3",
    "typescript": "^3.0.3",
    "typestyle": "^2.0.1",
    "url-join": "^4.0.0",
    "utility-types": "3.1.0",
    "webpack": "4.28.2",
    "webpack-cli": "^3.1.0",
    "copy-webpack-plugin": "^4.5.3",
    "write-file-webpack-plugin": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-info": "^4.1.3",
    "@storybook/react": "^4.0.0-alpha.24",
    "@types/enzyme": "3.1.15",
    "@types/jest": "23.3.10",
    "@types/webpack-dev-server": "3.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.4",
    "dependency-cruiser": "^4.6.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "jest": "23.6.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-hot-loader": "^4.3.8",
    "ts-jest": "23.10.5",
    "typescript-eslint-parser": "^21.0.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.0"
  }
}
