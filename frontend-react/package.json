{
  "name": "devshop-kea",
  "version": "0.1.0",
  "description": "Devshop with Kea.js",
  "author": "omelhoro",
  "homepage": "https://devshop.software-unchained.com",
  "repository": "https://github.com/omelhoro/devshop",
  "bugs": "https://github.com/omelhoro/devshop/issues",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "source vars-prod.sh && parcel build index.html --public-url /react-kea && npm run set:base-url",
    "set:base-url": "sed -i -e 's/base href=\"\"/base href=\"\\/react-kea\"/g' dist/index.html",
    "dev": "source vars-dev.sh && parcel index.html",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:watch": "jest --config configs/jest.json --watch",
    "test:update": "jest --config configs/jest.json -u",
    "verify": "npm run lint & yarn run tsc & yarn run test",
    "graph": "tsc && madge --image graph.svg out/index.js --warning"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@types/lodash": "^4.14.123",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "7.0.8",
    "@types/react-router-dom": "4.3.2",
    "@types/redux-form": "^8.1.1",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.4.0",
    "history": "^4.9.0",
    "jquery": "^3.4.0",
    "kea": "^0.28.7",
    "kea-saga": "^0.3.5",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-snackbar": "^0.1.15",
    "parcel-bundler": "^1.12.3",
    "popper.js": "^1.15.0",
    "query-string": "^6.5.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "6",
    "react-router-dom": "^5.0.0",
    "redux": "4.0.1",
    "redux-saga": "^1.0.2",
    "reselect": "4.0.0",
    "rimraf": "2.6.3",
    "styled-components": "^4.2.0",
    "tslib": "1.9.3",
    "typescript": "^3.4.5",
    "typestyle": "^2.0.2",
    "url-join": "^4.0.0",
    "utility-types": "3.5.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/enzyme": "3.9.1",
    "@types/jest": "24.0.11",
    "babel-eslint": "10.0.1",
    "dependency-cruiser": "^4.16.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "jest": "24.7.1",
    "ts-jest": "24.0.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
