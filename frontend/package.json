{
  "name": "react-redux-typescript-webpack-starter",
  "version": "0.1.0",
  "description": "React / Redux / Typescript / Webpack - Starter",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (https://piotrwitek.github.io/)",
  "homepage": "https://piotrwitek.github.io/react-redux-typescript-webpack-starter/",
  "repository": "https://github.com/piotrwitek/react-redux-typescript-webpack-starter.git",
  "bugs": "https://github.com/piotrwitek/react-redux-typescript-webpack-starter/issues",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "source vars-dev.sh && webpack-dev-server --config configs/webpack.config.js",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js",
    "dev:awesome": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome",
    "lint": "eslint --parser typescript-eslint-parser . --ext tsx --ext ts --ext js",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:watch": "jest --config configs/jest.json --watch",
    "test:update": "jest --config configs/jest.json -u",
    "reinstall": "rm -rf node_modules && npm",
    "reinstall:win": "rd /s /q node_modules && npm",
    "precommit": "npm run lint",
    "prepush": "npm run lint & yarn run tsc & yarn run test",
    "deploy": "echo 'deploy not set!'",
    "graph": "tsc && madge --image graph.svg out/index.js --warning"
  },
  "dependencies": {
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-hot-loader": "3.0.6",
    "@types/react-redux": "5.0.15",
    "@types/react-router-dom": "4.2.4",
    "@types/react-router-redux": "5.0.12",
    "@types/redux-form": "^7.2.0",
    "@types/webpack": "3.8.8",
    "@types/webpack-env": "1.13.5",
    "awesome-typescript-loader": "^3.5.0",
    "bootstrap": "^4.0.0",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "3.0.4",
    "jquery": "^3.3.1",
    "kea": "^0.28.2",
    "kea-saga": "^0.3.2",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "node-snackbar": "^0.1.9",
    "popper.js": "^1.12.9",
    "postcss-loader": "^2.1.1",
    "query-string": "^5.1.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-form": "^7.3.0",
    "redux-saga": "^0.16.0",
    "reselect": "3.0.1",
    "rimraf": "2.6.2",
    "ts-loader": "4.0.1",
    "tslib": "1.9.0",
    "typescript": "^2.8.0-dev.20180302",
    "typestyle": "^1.7.2",
    "url-join": "^4.0.0",
    "utility-types": "1.0.0",
    "webpack": "4",
    "webpack-cli": "^2.0.10",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.9",
    "@types/jest": "22.1.4",
    "dependency-cruiser": "^3.0.0",
    "@types/webpack-dev-server": "2.9.3",
    "babel-eslint": "8.2.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "22.4.2",
    "react-hot-loader": "^4.0.0",
    "ts-jest": "22.4.1",
    "typescript-eslint-parser": "^14.0.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.1"
  }
}
